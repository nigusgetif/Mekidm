<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Club Creation</title>
  <link rel="stylesheet" href="/nav/adminNav.css">
  <link rel="stylesheet" href="/assets/css/admin/Clubs.css">


</head>
<body>

<%- include('../partials/adminnav') %>
<div class="body">
  <div class="creationReq">
    <h1>Club Creation</h1>
    <p>List of Clubs waiting for approval to be created:</p>
<table id="club">
   <tr>
     <th>Club Name</th>
     <th>status</th>
     <th>Foundation Date</th>
     <th>Email</th>
     <th>Goal</th>
     <th>Vission</th>
     <th>Mission</th>
     <th> Admin approval </th>
   </tr>
   
   <% if (club !== 0) { %>
      <% club.forEach( club => { %>
      <tr>
        <td> <%= club.ClubName %>  </td>
        <td> <%= club.Status %>  </td>
        <td> <%= club.FoundationDate %>  </td>
        <td> <%= club.Email %>  </td>
        <td> <%= club.Goal %>  </td>
        <td> <%= club.Vision%>  </td>
        <td> <%= club.Mission%>  </td>
        <td><a href="approval/<%= club.ClubName %>">approve</a></td> 
      
      </tr>
      <% }) %>
    </table>
    <% } else { %>
    </table>
    <p> There is no club in the university at the moment </p>
   
    <%  } %>


 
</table>
</div>
</div>
<script>

  var icon =document.getElementById("icon");
  icon.onclick= function(){
    document.body.classList.toggle("dark-theme");
    if (document.body.classList.contains("dark-theme")){
      meklog.src="/assets/images/logo3.png";
      landinglogo.src="/assets/images/logo4.png";

    
    } else{
      meklog.src="/assets/images/logo.png";
      landinglogo.src="/assets/images/logo2.png";
      
    }
  
  }
  
    </script>

</body>
<footer class="footer">
      
  <div class="container">
    
    <div class="footer_bottom"><small class="end">Copyright Â© 2021 by Mekdim. All rights reserved.</small> </div>
    
</div>
  

</footer>
</html>


