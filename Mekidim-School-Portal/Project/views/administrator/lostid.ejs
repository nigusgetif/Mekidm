<!DOCTYPE html>
<html>
<head>

<meta charset="utf-8">
    <title>Lost Id</title>
    <link rel="stylesheet" href="/nav/adminNav.css">
    <link rel="stylesheet" href="/assets/css/admin/LostId.css">

</head>
<body>
<%- include('../partials/adminnav') %>
<div class="body">
  <h1>Lost ID Requests</h1><br>
    <p>Display of requests for lost ID:</p><br>
<table id="lostid">
   <tr>
     <th>Student Id</th>
     <th>Name</th>
     <th>Current year</th>
     <th>Department</th>
     <th>Date lost</th>
     <th>Status</th>
     <th> Admin approval </th>
   </tr>
   
   <% if (result !== 0) { %>
      <% result.forEach( result => { %>
      <tr>
        <td> <%= result.sId %>  </td>
        <td> <%= result.name %>  </td>
        <td> <%= result.currentyear %>  </td>
        <td> <%= result.Department %>  </td>
        <td> <%= result.datelost %>  </td>
        <td> <%= result.status%>  </td>
        <td><a href="lostid/<%= result.name %>">approve</a></td> 
      
      </tr>
      <% }) %>
    </table>
    <% } else { %>
    </table>
    <p> There is no lost id  in the university at the moment </p>
   
    <%  } %>
 
</table>
</div>

<script>

  var icon =document.getElementById("icon");
  icon.onclick= function(){
    document.body.classList.toggle("dark-theme");
    if (document.body.classList.contains("dark-theme")){
      meklog.src="/assets/images/logo3.png";
      landinglogo.src="/assets/images/logo4.png";

    
    } else{
      meklog.src="/assets/images/logo.png";
      landinglogo.src="/assets/images/logo2.png";
      
    }
  
  }
  
    </script>

</body>
<footer class="footer">
      
  <div class="container">
    
    <div class="footer_bottom"><small class="end">Copyright Â© 2021 by Mekdim. All rights reserved.</small> </div>
    
</div>
  

</footer>


</html>


