      <%- include('../partials/studentnav') %>
     <br>
   
    <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/assets/css/student/viewclub.css">
    <title>view</title>
</head>
<body>
  <table id="customers">
    <tr>
      <th>Club Name</th>
      <th>Foundation Date</th>
      <th>Email</th>
      <th>Goal</th>
      <th>Vission</th>
      <th>Mission</th>
    </tr>
    
    <% if (club !== 0) { %>
       <% club.forEach( club => { %>
       <tr>
         <td> <%= club.ClubName %>  </td>
         <td> <%= club.FoundationDate %>  </td>
         <td> <%= club.Email %>  </td>
         <td> <%= club.Goal %>  </td>
         <td> <%= club.Vision%>  </td>
         <td> <%= club.Mission%>  </td>
       </tr>
       <% }) %>
     </table>
     <% } else { %>
     </table>
     <p> There is no club in the university at the moment </p>
    
     <%  } %>
  
 </table>
    
</body> 
</html>


    <footer class="footer">
      
      <div class="container">
        
        <div class="footer_bottom"><small class="end">Copyright Â© 2021 by Mekdim. All rights reserved.</small> </div>
        
    </div>
      
    </footer>



    <script>

      var icon =document.getElementById("icon");
      icon.onclick= function(){
        document.body.classList.toggle("dark-theme");
        if (document.body.classList.contains("dark-theme")){
          meklog.src="/assets/images/logo3.png";
        
        } else{
          meklog.src="/assets/images/logo.png";
          
        }
      
      }
      
        </script>
  </body>
</html>
