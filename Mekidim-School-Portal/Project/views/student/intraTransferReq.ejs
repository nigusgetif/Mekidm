    <link rel="stylesheet" href="/assets/css/student/intratransfer_application.css">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous"/>


  </head>
  <body>
    
    <%- include('../partials/studentnav') %>

  <section class="section">
        <h1><pre>       Intratransfer Application</pre></h1><br>						<!--main function of page-->
        <p><pre>             Fill in this form to apply for intercollege transfer:</pre></p><br><br>
        <div id="div_a">
        <form action="/intraTransferReq" method="post">
          <label for="fname">First Name:</label>		                             	<!--first name information-->
             <input type="text" id="fname" name="fname" value=""required><br><br>
          <label for="date">Date of Birth:</label>		                              	<!--date of birth information-->
             <input type="text" id="date" name="date" placeholder="dd/mm/yyyy" required><br><br>
          <label for="sex">Sex:</label>			        	                              <!--Sex information-->
             <input type="text" id="sex" name="sex" value="" required><br><br>
          <label for="depfrom">Department from:</label>	                       	<!--department from information-->
             <input type="text" id="depfrom" name="depfrom" value="" required><br><br>
          <label for="egy">Expected Graduation Year:</label>		                      <!--graduation year information-->
             <input type="text" id="egy" name="egy" value="" required><br><br>
        </div>
        <div id="div_b">
          <label for="mname">Middle Name:</label>		                   	<!--middle name information-->	
             <input type="text" id="mname" name="mname" value="" required><br><br>
          <label for="id">ID:</label>				                   	<!--ID information-->
             <input type="text" id="id" name="id" value="" required><br><br>
          <label for="cgpa">CGPA:</label>			                 	<!--cgpa information-->
             <input type="text" id="cgpa" name="cgpa" value="" required><br><br>
          <label for="depto">Department to:</label>		             	<!--department to information-->
             <input type="text" id="depto" name="depto" value="" required><br><br>
        </div>
        <div id="div_c">
          <label for="lname">Last Name:</label>			<!--last name information-->
              <input type="text" id="lname" name="lname" value="" required><br><br><br><br><br><br><br><br><br><br><br>
          <input type="submit" value="Submit">				<!--submit button-->
        </div>           
        </form>
      </div>
  </section>

