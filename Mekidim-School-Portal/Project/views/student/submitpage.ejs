<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title> Document</title>
	<link rel="stylesheet" href="/assets/css/assignmentSubmission.css" type="text/css">
	<link rel="stylesheet" href="/nav/studentNav.css">
</head>
<body>
    <%- include('../partials/studentnav') %>
    <form action="/projandassSubmition" method="post" enctype="multipart/form-data">
        <% if (msg == null){ %>
                
                <p>NO courses.</p>
            <% }else {%>
                <select name="courses" id="cars">
                <% msg.forEach( cmsg => {%>
                    <% if (cmsg != null){ %>
                        <option value=<%= cmsg.courseName %>><%= cmsg.courseName%></option>
                    <% }%>
                <%})%>
                </select>
            <% } %>
        <input type="file" name="assFile" id="sub">
        <input type="submit" name="subButton" value="Upload">
    </form>

    <footer class="footer">
      
        <div class="container">
          
          <div class="footer_bottom"><small class="end">Copyright Â© 2021 by Mekdim. All rights reserved.</small> </div>
          
      </div>
        
      </footer>
      
       <script>
  
        var icon =document.getElementById("icon");
        icon.onclick= function(){
          document.body.classList.toggle("dark-theme");
          if (document.body.classList.contains("dark-theme")){
            meklog.src="/assets/images/logo3.png";
          
          } else{
            meklog.src="/assets/images/logo.png";
            
          }
        
        }
        
          </script>
</body>
</html>